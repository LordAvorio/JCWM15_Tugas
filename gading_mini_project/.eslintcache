n//#region LifeCycle\r\n\r\ncomponentDidMount() {\r\n    Axios.get(\"http://localhost:2000/users\")\r\n      .then((res) => {\r\n        this.setState({ dbUsers: res.data });\r\n      })\r\n      .catch((err) => {\r\n        console.log(err);\r\n      });\r\n  }\r\n\r\n//#endregion\r\n \r\n//#region Component\r\n\r\n    tableHead = () => {\r\n        return (\r\n        <thead >\r\n            <tr id=\"theadJsonTable\">\r\n                <th>#</th>\r\n                <th>First Name</th>\r\n                <th>Last Name</th>\r\n                <th>Email</th>\r\n                <th>Actions</th>\r\n            </tr>\r\n        </thead>\r\n        );\r\n    };\r\n\r\n    tableBody = () => {\r\n        let { dbUsers } = this.state;\r\n        return (\r\n        <tbody>\r\n            {dbUsers.map((item, index) => {\r\n            return (\r\n                <tr key={index} id=\"tBodyJsonTable\">\r\n                    <td>{index + 1}</td>\r\n                    <td>{item.first_name}</td>\r\n                    <td>{item.last_name}</td>\r\n                    <td>{item.email}</td>\r\n                    <td>\r\n                        <Button  className=\"mx-2\"  variant=\"primary\" onClick={() => this.handleEditData(index)}>Edit</Button>\r\n                        <Button  className=\"mx-2\"  variant=\"danger\" onClick={() => this.handleDeleteData(index)}>Delete</Button>\r\n                    </td>\r\n                </tr>\r\n            );\r\n            })}\r\n        </tbody>\r\n        );\r\n    };\r\n\r\n    tableInput = () => {\r\n        return (\r\n        <tbody>\r\n            <tr>\r\n            <td>#</td>\r\n            <td>\r\n                <Form.Control\r\n                type=\"text\"\r\n                placeholder=\"Masukkan Nama Depan\"\r\n                value={this.state.first_name}\r\n                name=\"first_name\"\r\n                onChange={(event) => this.handleChange(event)}\r\n                />\r\n            </td>\r\n            <td>